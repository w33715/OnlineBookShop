<Window x:Class="Generating_Excel_Files.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Generating_Excel_Files"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid>
        <Button x:Name="btnExcel" Height="50" Width="50" HorizontalAlignment="Center" VerticalAlignment="Center"
                Click="Button_Click">
            <Button.Template>
                <ControlTemplate>
                    <Image Source="/Image/cd.png"/>
                </ControlTemplate>
            </Button.Template>
        </Button>
    </Grid>
</Window>
